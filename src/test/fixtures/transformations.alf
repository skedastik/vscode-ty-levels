foo(x = 2 * x + 14)

<Foo x="{{ 2 * x + 14 }}" y="{{ y + 4 }}" />
foo(x = 2 * x + 14, y = 4)

<Foo x="{{ math.cos(x) }}" />
foo(x=math.cos(x))

