<Foo />
foo()

<Foo irrelevant="" />
foo(irrelevant='')

<Foo x="" />
foo(x='')

<Foo cx="0" cz="0" x="0" y="0" z="0" xx="0" yy="0" zz="0" w="0" d="0" h="0" deltaY="0" />
foo(cx=0, cz=0, x=0, y=0, z=0, xx=0, yy=0, zz=0, w=0, d=0, h=0, deltaY="0" )

<Foo x="{{ x }}" />
