<Foo@ j="{{ @2@ }}"@ i="{{ 0 }}" />
<Foo i="{{ 0 }}"@ j="{{ @2@ }}"@ />
<Foo i="{{ 0 }}"@ j="{{ @2@ }}"@ k="{{ 0 }}" />
<Foo
    i="{{ 0 }}"
   @ j="{{ @2@ }}"@
    k="{{ 0 }}"
/>
<Foo@ j="{{@2@}}"@ />
<Foo@ j="   {{   @2@   }}   "@ />

